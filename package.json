{
  "name": "kourou",
  "description": "The CLI that helps you manage your Kuzzle instances",
  "version": "1.0.0",
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "bin": {
    "kourou": "./bin/run"
  },
  "bugs": "https://github.com/kuzzleio/kourou/issues",
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "dev": "./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "test:lint": "eslint src/ tests/ --ext .ts",
    "test:lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "test:functional": "npm run build && npm run test:functional:stdout && npm run test:functional:cucumber",
    "test:functional:stdout": "nyc --extension .ts mocha --forbid-only \\\"tests/**/*.test.ts\\\"",
    "test:functional:cucumber": "./node_modules/.bin/cucumber-js --fail-fast",
    "test:functional:jest": "jest --runInBand"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.12.0",
    "@oclif/command": "1.8.*",
    "@oclif/config": "1.18.*",
    "@oclif/plugin-autocomplete": "1.3.*",
    "@oclif/plugin-help": "3.2.2",
    "@types/chai": "^4.3.4",
    "bluebird": "^3.7.2",
    "chai": "^4.3.7",
    "chalk": "^4.1.0",
    "chrono-node": "^2.4.2",
    "cli-ux": "^5.5.1",
    "inquirer": "^8.0.0",
    "kepler-companion": "^1.1.3",
    "kuzzle-sdk": "^7.10.1",
    "kuzzle-vault": "^2.0.4",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "ndjson": "^2.0.0",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.7",
    "strip-json-comments": "^3.1.1",
    "tar": "^6.1.11",
    "tmp": "^0.2.1",
    "tslib": "^2.3.1",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/bluebird": "^3.5.33",
    "@types/compare-version": "^0.1.31",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^29.4.0",
    "@types/listr": "^0.14.2",
    "@types/ndjson": "^2.0.0",
    "@types/node": "^14.14.41",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.6.1",
    "@types/tar": "^6.1.3",
    "@types/tmp": "^0.2.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cucumber": "^6.0.7",
    "eslint": "^7.24.0",
    "globby": "^11.0.3",
    "jest": "^29.4.1",
    "nyc": "^15.1.0",
    "prettier-eslint": "^12.0.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.19",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.*",
    "typescript": "4.4.*"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kuzzleio/kourou",
  "keywords": [
    "kuzzle",
    "CLI"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kourou",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "repositoryPrefix": "<%- commandPath %>",
    "topics": {
      "api-key": {
        "description": "manage API keys"
      },
      "app": {
        "description": "create, manage and run applications"
      },
      "collection": {
        "description": "create an entire data collection"
      },
      "document": {
        "description": "push or retrieve documents from the data storage"
      },
      "es": {
        "description": "low-level access to Elasticsearch"
      },
      "export": {
        "description": "export many data from Kuzzle"
      },
      "vault": {
        "description": "manage then encrypted vault"
      },
      "sdk": {
        "description": "directly manipulate the sdk"
      }
    },
    "hooks": {
      "command_not_found": "./lib/hooks/command_not_found/api-action"
    }
  },
  "repository": "kuzzleio/kourou",
  "types": "lib/index.d.ts"
}
