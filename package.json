{
  "name": "kourou",
  "description": "The CLI that helps you manage your Kuzzle instances",
  "version": "1.0.0",
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "bin": {
    "kourou": "./bin/run"
  },
  "bugs": "https://github.com/kuzzleio/kourou/issues",
  "scripts": {
    "build": "tsc -b",
    "dev": "./bin/run",
    "postpack": "rm -f oclif.manifest.json",
    "version": "oclif-dev readme && git add README.md",
    "test:lint": "eslint src/ test/ --ext .ts",
    "test:lint:fix": "eslint src/ test/ --ext .ts --fix",
    "test:functional": "npm run build && npm run test:functional:stdout && npm run test:functional:cucumber",
    "test:functional:stdout": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test:functional:cucumber": "./node_modules/.bin/cucumber-js --fail-fast"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.12.0",
    "@oclif/command": "1.8.*",
    "@oclif/config": "1.18.*",
    "@oclif/plugin-autocomplete": "1.3.*",
    "@oclif/plugin-help": "3.2.2",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "cli-ux": "^5.6.7",
    "kepler-companion": "^1.1.5",
    "kuzzle-sdk": "^7.10.3",
    "kuzzle-vault": "^2.0.4",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "ndjson": "^2.0.0",
    "node-emoji": "^1.11.0",
    "node-fetch": "^3.2.10",
    "strip-json-comments": "^3.1.1",
    "tmp": "^0.2.1",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^2.2.9",
    "@types/bluebird": "^3.5.37",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^10.0.0",
    "@types/ndjson": "^2.0.1",
    "@types/node": "^14.18.33",
    "@types/node-emoji": "^1.8.2",
    "@types/node-fetch": "^2.6.2",
    "@types/tmp": "^0.2.3",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cucumber": "^6.0.7",
    "eslint": "^7.24.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.21",
    "ts-node": "10.9.*",
    "typescript": "4.4.*"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kuzzleio/kourou",
  "keywords": [
    "kuzzle",
    "CLI"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "kourou",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "repositoryPrefix": "<%- commandPath %>",
    "topics": {
      "api-key": {
        "description": "manage API keys"
      },
      "app": {
        "description": "create, manage and run applications"
      },
      "collection": {
        "description": "create an entire data collection"
      },
      "document": {
        "description": "push or retrieve documents from the data storage"
      },
      "es": {
        "description": "low-level access to Elasticsearch"
      },
      "export": {
        "description": "export many data from Kuzzle"
      },
      "vault": {
        "description": "manage then encrypted vault"
      },
      "sdk": {
        "description": "directly manipulate the sdk"
      }
    },
    "hooks": {
      "command_not_found": "./lib/hooks/command_not_found/api-action"
    }
  },
  "repository": "kuzzleio/kourou",
  "types": "lib/index.d.ts"
}
