{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "object-curly-spacing": "off",
    "comma-dangle": "off",
    "indent": "off",
    "no-await-in-loop": "off",
    "node/no-deprecated-api": "off",
    "unicorn/no-abusive-eslint-disable": "off",
    "brace-style": "off",
    "operator-linebreak": "off"
  }
}
